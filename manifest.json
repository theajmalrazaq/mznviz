{
  "name": "mznviz Finance",
  "short_name": "mznviz",
  "description": "Professional Meezan Bank Statement Visualizer",
  "id": "/mznviz/",
  "start_url": "/mznviz/",
  "scope": "/mznviz/",
  "display": "standalone",
  "background_color": "#1B1B1B",
  "theme_color": "#1B1B1B",
  "categories": ["finance"],
  "orientation": "portrait",
  "icons": [
    {
      "src": "favicon.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "favicon.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "favicon.svg",
      "sizes": "927x927",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "file_handlers": [
    {
      "action": "/mznviz/",
      "name": "Meezan Statement",
      "accept": {
        "text/csv": [".csv"],
        "text/comma-separated-values": [".csv"],
        "application/vnd.ms-excel": [".csv", ".xlsx"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "application/octet-stream": [".csv", ".xlsx"]
      },
      "icons": [
        {
          "src": "favicon.svg",
          "sizes": "927x927",
          "type": "image/svg+xml"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
}
