{
  "name": "mznviz Finance",
  "short_name": "mznviz",
  "description": "Professional Meezan Bank Statement Visualizer",
  "id": "/mznviz/",
  "start_url": "/mznviz/",
  "scope": "/mznviz/",
  "display": "standalone",
  "background_color": "#000000",
  "theme_color": "#000000",
  "categories": ["finance"],
  "orientation": "portrait",
  "icons": [
    {
      "src": "favicon.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "favicon.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "favicon.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "maskable"
    },
    {
      "src": "favicon.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "maskable"
    }
  ],
  "file_handlers": [
    {
      "action": "/mznviz/",
      "name": "Import to mznviz",
      "accept": {
        "text/csv": [".csv"],
        "text/comma-separated-values": [".csv"],
        "application/vnd.ms-excel": [".csv"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
}
